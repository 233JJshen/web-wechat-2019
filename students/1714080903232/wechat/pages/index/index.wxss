.music{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  position: fixed;
  top: 40rpx;
  right: 40rpx;
  z-index: 9999;
}

.on{
  animation:rotating 2.5s linear infinite;
}  

@keyframes rotating{
from{transform:rotate(0)}
to{transform:rotate(360deg)}
}

